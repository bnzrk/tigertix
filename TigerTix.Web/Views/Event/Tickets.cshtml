@model List<Ticket>

<h1> Tickets </h1>
<table>
    <tr>
        <th>Section&emsp;</th>
        <th>Row&emsp;</th>
        <th>Seat Number&emsp;</th>
        <th>Price&emsp;</th>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.Section&emsp;</td>
            <td>@t.Row&emsp;</td>
            <td>@t.SeatNumber&emsp;</td>
            <td>@t.Price&emsp;</td>
            <td>
            <form asp-action="ClaimTicket" method="post" asp-route-ticketId="@t.Id">
                <input type="submit" value="Claim Ticket" />
            </form>
            </td>
        </tr>
    }
</table>