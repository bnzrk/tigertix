@model TicketViewModel
@{

    ViewData["Title"] = "Ticket Creation";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>TigerTix: @ViewData["Title"]</title>
</head>
<body>
    <h1>Event Info</h1>

    <form method="post">
        <label>CUID:</label>
        <br />
        <input type="text" name="CUID" />
        <br />
        <label>Event Name:</label>
        <br />
        <input type="text" name="TicketEvent" />
        <br />
        <label>Seat Number:</label>
        <br />
        <input type="number" name="SeatNumber" />
        <br />
        <input type="submit" value="Create" />
    </form>
</body>
</html>