@model RegisterViewModel
@{

    ViewData["Title"] = "Profile";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>TigerTix: @ViewData["Title"]</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter+Tight:ital,wght@0,100..900;1,100..900&family=Inter:ital,opsz,wght@0,14..32,100..900;1,14..32,100..900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <link href="~/css/main.css" rel="stylesheet">
</head>
<body class="focus-container-background">
    <!-- Start of Banner -->
    <div id="TopBanner">
        <div id="TopBannerLeft">
            <div class="tt-logo">
                <a href="/App/Index" , class="NoHyperlinkUnderline">
                    <span class="tt-logo-tiger">Tiger</span><span class="tt-logo-tix">Tix</span>
                </a>
            </div>
        </div>
        <div id="TopBannerRight">
            <a href="/App/Login" class="NoHyperlinkUnderline TopRightLink">Login</a>
            <a href="/App/Register" class="NoHyperlinkUnderline TopRightLink">Register</a>
            <a href="/App/Profile" class="NoHyperlinkUnderline TopRightLink">My Account</a>
        </div>
    </div>
    <!-- End of Banner -->

    <div class="focus-container ProfileContainer">
        <h1> Profile </h1>
        <h2> CUID </h2>
        <p id="cuid"> C123456789 </p>
        <h2> First Name </h2>
        <p id="firstname"> John </p>
        <h2> Last Name </h2>
        <p id="lastname"> Smith </p>
        <h2> Email </h2>
        <p id="email"> emailplaceholder.email.com </p>
        <h2> Password </h2>
        <p id="password"> ******** </p>
        <a href="/App/EditProfile" class="">Edit Profile</a>
    </div>
</body>
</html>
